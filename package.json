{
  "name": "contract-ledger-midway",
  "version": "1.0.0",
  "description": "客户合同管理系统 - Midway版本",
  "scripts": {
    "test-api": "node test-api.js",
    "test-login": "node test-login.js",
    "start": "start-simple.bat",
    "start-ps": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "install-all": "cd midway-backend && yarn install && cd ../midway-frontend && yarn install",
    "build-all": "cd midway-backend && yarn build && cd ../midway-frontend && yarn build",
    "clean": "rimraf */node_modules */yarn.lock yarn.lock node_modules",
    "dev": "start-simple.bat",
    "dev-ps": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "apply-indexes": "node scripts/apply-mysql-indexes.js",
    "test-db": "node scripts/apply-mysql-indexes.js --test",
    "db-info": "node scripts/apply-mysql-indexes.js --info",
    "performance-test": "node scripts/performance-test.js"
  },
  "keywords": [
    "contract",
    "management",
    "midway",
    "vue3",
    "typescript"
  ],
  "author": "Midway Team",
  "license": "MIT",
  "devDependencies": {
    "axios": "^1.6.0"
  }
}
